  <POU Name="MAIN" Id="{c8ef33a4-305b-4802-9860-82a055d32b0a}" SpecialFunc="None">
    <Declaration><![CDATA[PROGRAM MAIN
VAR
	
	{attribute 'TcLinkTo' := '
		.bLimitForwardEnable	:= TIID^Device 1 (EtherCAT)^E20 (EK1100)^E29 (EL1088)^Channel 1^Input;
		.bLimitBackwardEnable	:= TIID^Device 1 (EtherCAT)^E20 (EK1100)^E29 (EL1088)^Channel 3^Input'}
	MJetX	: DUT_MotionStage;
	
	{attribute 'TcLinkTo' := '
		.bLimitForwardEnable	:= TIID^Device 1 (EtherCAT)^E20 (EK1100)^E29 (EL1088)^Channel 5^Input;
		.bLimitBackwardEnable	:= TIID^Device 1 (EtherCAT)^E20 (EK1100)^E29 (EL1088)^Channel 7^Input'}
	MJetY	: DUT_MotionStage;
	
	{attribute 'TcLinkTo' := '
		.bLimitForwardEnable	:= TIID^Device 1 (EtherCAT)^E20 (EK1100)^E29 (EL1088)^Channel 2^Input;
		.bLimitBackwardEnable	:= TIID^Device 1 (EtherCAT)^E20 (EK1100)^E29 (EL1088)^Channel 4^Input'}
